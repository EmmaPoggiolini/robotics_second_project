<launch>

  <node pkg="slam_toolbox" type="async_slam_toolbox_node" name="slam_toolbox" output="screen">
    <rosparam command="load" file="$(find second_project)/cfg/st_config_scan.yaml" />
  </node>

  <!-- <include file="$(find second_project)/launch/scan_convertion.launch"/> -->
  <include file="$(find second_project)/launch/single_lidar.launch"/>
  
   <!--  static tfs  -->  
  <node pkg="tf2_ros" type="static_transform_publisher" name="base_laser"  args=" 0 0 0  0 0 0 1 base_link base_laser_link "/>
  
 <!--  rviz  -->
 <node name="rviz" pkg="rviz" type="rviz" args="-d $(find second_project)/config_mapping.rviz"/>
 <include file="$(find second_project)/launch/second_project.launch"/>

</launch>
